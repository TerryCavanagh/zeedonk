function new(){
  Game.title = "Zeedonk Asset Pack";
  Text.setfont(Font.THIN);
}

function update(){
  Gfx.clearscreen(Col.NIGHTBLUE);
  for(j in 0 ... 4){
    for(i in 0 ... 10){
      Gfx.drawimage(4 + i*16, j*16, "tile0");
    }  
  }
  
  for(j in 0 ... 4){
    for(i in 0 ... 10){
      Gfx.drawimage(4 + i*16, 72 + j*16, "16r");
    }  
  }
  
  for(j in 0 ... 15){
    for(i in 0 ... 6){
      Gfx.drawimage(176 + i*9, j*9, icon[i + (j * 6)]);
    }  
  }
  Text.display(Text.LEFT + 5, Text.BOTTOM, "ZEEDONK ASSET PACK");
  Text.display(Text.RIGHT - 5, Text.BOTTOM, "FEEL FREE TO USE IN YOUR OWN GAMES");
}